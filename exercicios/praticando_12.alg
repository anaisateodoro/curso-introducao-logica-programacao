algoritmo "Escolha"

var
voto: literal

inicio
// Seção de Comandos
Escreval("Digite seu voto")
leia(voto)

se voto = "20" entao
   escreval("Você votou em Marcelo Bernat")
senao
se voto ="30" entao
   escreval(" Você votou no Pernalonga")
senao
se voto = "40" entao
   escreval(" Você votou no Patolino")
senao
     escreval("Voto nulo.")
fimse
fimse
fimse


fimalgoritmo
